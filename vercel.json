{
    "version": 2,
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python@3.1.0",
            "config": {
                "runtime": "python3.12"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/main.py"
        }
    ]
}